.record_item_container {
  display: flex;
  padding: 31rpx 24rpx 22rpx 0;
  border-bottom: 1rpx solid #ccc;
  justify-content: space-between;
}
.record_item_container .left_block {
  display: flex;
}
.record_item_container .left_block .record_level {
  color: #EBB22E;
  font-size: 36rpx;
}
.record_item_container .left_block .record_item_desc {
  color: #999;
  font-size: 28rpx;
  margin-left: 13rpx;
}
.record_item_container .left_block .record_item_desc .item_edsc {
  width: 350rpx;
}
.record_item_container .left_block .record_item_desc .item_edsc text {
  color: #333;
  font-weight: 500;
}
.record_item_container .record_amount {
  font-size: 28rpx;
  color: #666666;
  line-height: 40rpx;
}
