.product_box {
  padding: 40rpx 0;
  border-bottom: 1rpx solid #dfdfdf;
  display: flex;
}
.product_box .product_item_icon {
  width: 130rpx;
  height: 130rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 80rpx;
  flex-shrink: 0;
  border-radius: 50%;
  margin-right: 30rpx;
  margin-top: 10rpx;
}
.product_box .product_item_detail .product_item_title {
  font-size: 32rpx;
  font-weight: 500;
  color: #333333;
  line-height: 42rpx;
  margin-bottom: 20rpx;
}
.product_box .product_item_detail .product_item_desc {
  font-size: 26rpx;
  font-weight: 500;
  color: #666666;
  line-height: 33rpx;
  margin-bottom: 20rpx;
}
.product_box .product_item_detail .product_item_btn {
  width: 200rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  border-radius: 30rpx;
  justify-content: center;
  background: linear-gradient(330deg, #33c29d 0%, #4edaa9 100%);
  font-size: 24rpx;
  font-weight: 500;
  color: #ffffff;
  line-height: 33rpx;
}
