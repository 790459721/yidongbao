/* pages/clientManage/clientManage.wxss */
.client_manage_wrapper {
  width: 100vw;
  min-height: 100vh;
}
.client_manage_wrapper .client_search_box {
  width: 100%;
  height: 106rpx;
  background: #f1f1f1;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
}
.client_manage_wrapper .client_search_box .icon_search {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: contain;
  position: absolute;
  top: 35rpx;
  left: 76rpx;
}
.client_manage_wrapper .client_search_box .client_search_input {
  width: 680rpx;
  height: 66rpx;
  border-radius: 33rpx;
  background: #fff;
  padding-left: 91rpx;
}
.client_manage_wrapper .client_list {
  padding: 0 30rpx;
  padding-top: 106rpx;
  padding-bottom: 50rpx;
}
