/**index.wxss**/
.home_wrapper {
  background: #f6f6f6;
  min-height: 100vh;
  padding-bottom: 200rpx;
}
.home_wrapper .home_banner_back {
  width: 100%;
  height: 300rpx;
  background: url(http://minipropub.oss-cn-shenzhen.aliyuncs.com/mini-yidongbao/home_nav_back.png) no-repeat center;
  background-size: 100% 100%;
  position: relative;
}
.home_wrapper .home_banner_back .home_banner {
  width: 708rpx;
  height: 339rpx;
  background: #369;
  border-radius: 10rpx;
  position: absolute;
  top: 89rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.home_wrapper .home_main {
  padding: 0 20rpx;
  margin-top: 128rpx;
}
.home_wrapper .module_title {
  height: 92rpx;
  background: linear-gradient(180deg, #ffc7c7 0%, #f8f9fa 100%);
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  display: flex;
  align-items: center;
  color: #F08440;
  font-size: 38rpx;
  font-size: 500;
  padding-left: 19rpx;
}
.home_wrapper .module_title.insurance_title {
  margin-top: 55rpx;
  background: linear-gradient(180deg, #d0faee 0%, #f8f9fa 100%);
  color: #33C29D;
}
.home_wrapper .module_title.my_doctor {
  background: linear-gradient(180deg, rgba(250, 242, 223, 0.97) 0%, #f8f9fa 100%);
}
.home_wrapper .services_project_box {
  background: #fff;
  display: flex;
  height: 200rpx;
  background: #ffffff;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
  align-items: center;
  justify-content: space-around;
}
.home_wrapper .services_project_box .project_item {
  display: flex;
  flex-direction: column;
}
.home_wrapper .services_project_box .project_item .item_icon {
  width: 90rpx;
  height: 90rpx;
  margin-bottom: 17rpx;
}
.home_wrapper .services_project_box .project_item .item_icon.icon_client {
  background: url(https://minipropub.oss-cn-shenzhen.aliyuncs.com/mini-yidongbao/icon_client.png) no-repeat center;
  background-size: contain;
  margin-left: 15rpx;
}
.home_wrapper .services_project_box .project_item .item_icon.icon_my_card {
  background: url(https://minipropub.oss-cn-shenzhen.aliyuncs.com/mini-yidongbao/icon_my_card.png) no-repeat center;
  background-size: contain;
  margin-left: 10rpx;
}
.home_wrapper .services_project_box .project_item .item_icon.icon_public_active {
  background: url(https://minipropub.oss-cn-shenzhen.aliyuncs.com/mini-yidongbao/icon_public_active.png) no-repeat center;
  background-size: contain;
  margin-left: 17rpx;
}
.home_wrapper .services_project_box .project_item .item_name {
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.home_wrapper .services_project_box .project_split {
  height: 131rpx;
  width: 2rpx;
  background: #f7f7f7;
}
.home_wrapper .my_doctor_box {
  background: #fff;
  display: flex;
  height: 283rpx;
  background: #ffffff;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
  padding: 45rpx 17rpx;
}
.home_wrapper .my_doctor_box .my_doctor_left {
  margin-right: 27rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.home_wrapper .my_doctor_box .my_doctor_left .doctor_avatar {
  width: 120rpx;
  height: 128rpx;
  border-radius: 10rpx;
  background: #ccc;
  margin-bottom: 20rpx;
}
.home_wrapper .my_doctor_box .my_doctor_left .message_btn {
  width: 120rpx;
  height: 45rpx;
  background: #5adeb7;
  border-radius: 22rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FFFFFF;
  font-size: 28rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top {
  width: 520rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .right_top_left {
  display: flex;
  align-items: center;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .right_top_left .doctor_name {
  font-size: 46rpx;
  font-weight: 500;
  color: #333333;
  line-height: 65rpx;
  color: #333;
  margin-right: 27rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .right_top_left .doctor_job_title {
  font-size: 28rpx;
  color: #999999;
  line-height: 40rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .like_state {
  width: 140rpx;
  height: 45rpx;
  border-radius: 23rpx;
  border: 2rpx solid #999999;
  display: flex;
  align-items: center;
  padding-left: 13rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .like_state .like_icon {
  font-size: 26rpx;
  color: #999999;
  margin-right: 12rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_top .like_state .like_text {
  font-size: 26rpx;
  color: #999999;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_center {
  margin: 1rpx 0 14rpx;
  display: flex;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_center .right_center_hospital {
  margin-right: 20rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom {
  display: flex;
  align-items: center;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .bottom_tag {
  width: 160rpx;
  height: 44rpx;
  border-radius: 4rpx;
  border: 1rpx solid #cccccc;
  display: flex;
  align-items: center;
  margin-right: 46rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .bottom_tag .tag_point {
  font-size: 20rpx;
  color: #33C29D;
  margin: 0 17rpx 0 11rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .bottom_tag .tag_text {
  font-size: 26rpx;
  color: #999999;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .bottom_star_icon {
  font-size: 30rpx;
  color: #5ADEB7;
  margin-right: 11rpx;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .bottom_star_num {
  margin-right: 12rpx;
  font-size: 26rpx;
  font-family: ZhenyanGB-Regular, ZhenyanGB;
  font-weight: 700;
  color: #f6b631;
}
.home_wrapper .my_doctor_box .my_doctor_right .right_bottom .message_num {
  font-size: 26rpx;
  color: #999999;
}
.home_wrapper .empty_box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999999;
}
.home_wrapper .empty_box .empty_icon {
  width: 81rpx;
  height: 81rpx;
  background: #ccc;
  margin-bottom: 34rpx;
}
.home_wrapper .empty_box .empty_message {
  font-size: 28rpx;
  font-weight: 500;
}
.home_wrapper .empty_box .empty_message text {
  color: #33C29D;
}
.home_wrapper .product_list_box {
  background: #fff;
  padding: 0 30rpx;
}
